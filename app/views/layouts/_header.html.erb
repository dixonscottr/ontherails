<nav>
  <div id='mta-timestamp' id="update_time">
      <p class="small">Last update from MTA: <%= Time.now.strftime("%m/%d/%Y %H:%M") %></p>
  </div>
  <div class="nav-wrapper">

    <a class="brand-logo"><img src="img/rails.jpg" width="150px"/></a>
    <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="large material-icons">menu</i></a>
    <ul class="side-nav" id="mobile-demo">
      <% lines = { 1 => "one", 2 => "two", 3 => "three", 4 => "four", 5 => "five", 6 => "six" } %>
      <% lines.each do |line , name| %>
        <div class="col-sm-2">
          <li><a class="btn btn-circle raised <%= lines[line] %> checked center-block" onclick="handleClick(this)" name="line" value="<%=line.to_s%>"><%=line.to_s%></a>
          <span id='<%=line%>-service' class="serviceStatus">On Time
          </span>
          </li>
        </div>
      <% end %>
    </ul>
  </div>
</nav>
